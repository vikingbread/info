<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="newspackage" extends="default" >
		<action name="listNews" class="newsChannle" method="listNews">
			<result>/newsChannel/newsIndex.jsp</result>
		</action> 
		<action name="queryNewsById" class="newsChannle" method="queryNewsById">
			<result>/newsChannel/newsContent.jsp</result>
		</action>
		<action name="queryNewsListByModuleId" class="newsChannle" method="queryNewsListByModuleId">
			<result>/newsChannel/newsList.jsp</result>
		</action>
	</package>
</struts>